{
  "name": "astro-react-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter website dev",
    "build": "turbo run build",
    "build:ui": "pnpm --filter @astro-react/ui-button build",
    "dev:ui": "pnpm --filter @astro-react/ui-button dev",
    "dev:all": "pnpm build:ui && pnpm dev"
  },
  "dependencies": {},
  "devDependencies": {
    "turbo": "^1.10.0"
  }
}
